{
  "name": "@aulgn/builder",
  "version": "0.0.1",
  "description": "Plugin builder",
  "keywords": [
    "builder",
    "api"
  ],
  "homepage": "https://github.com/aulgn/builder#readme",
  "bugs": {
    "url": "https://github.com/aulgn/builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aulgn/builder.git"
  },
  "license": "MIT",
  "author": "aulgn",
  "type": "module",
  "main": "./dist/aulgn-builder.cjs",
